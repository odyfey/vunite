{
  "name": "vunite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run build && node scripts/serve",
    "build": "npm run build:server && rename-files ./dist vue-ssr-server-bundle.json ../bundle && npm run build:client && rename-files ./ bundle ./dist/vue-ssr-server-bundle.json",
    "build:client": "vue-cli-service build",
    "build:server": "cross-env WEBPACK_TARGET=node vue-cli-service build",
    "start": "cross-env NODE_ENV=production node scripts/start",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/vue-fontawesome": "^0.0.23",
    "axios": "^0.18.0",
    "element-ui": "^2.3.9",
    "emoji-mart-vue": "^2.6.3",
    "express": "^4.16.3",
    "mavon-editor": "^2.5.9",
    "moment": "^2.22.2",
    "pangu": "^3.3.0",
    "vue": "^2.5.16",
    "vue-affix": "^0.2.4",
    "vue-infinite-scroll": "^2.0.2",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "cross-env": "^5.2.0",
    "lodash.merge": "^4.6.1",
    "vue-template-compiler": "^2.5.16",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
